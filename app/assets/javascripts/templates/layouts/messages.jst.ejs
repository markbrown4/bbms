<h1>Messages</h1>
<a href="/messages/new" class="new btn" data-route>New Thread</a>
<ul id="threads">
<% _.each(threads, function(thread) { %>
  <%= JST['templates/threads/item'](thread) %>
<% }) %>
</ul>
<div id="thread">
  <h2>Inbox zero</h2>
  <p>Make a friend, <a href="/messages/new" data-route>Send someone a message</a>
</div>